<div id="userID" data-id="{{userID}}"></div>

<div id="failCont" class="container d-none">
    <div class="row justify-content-center bg-danger ">
        <div id="failResult" class=" col-xs text-light bd-danger">
            <h1> Hey no new Users for you to match with yet! Try again Later</h1>
        </div>
    </div>
    <div id="noMatchimg" class="row justify-content-center">
        <img src="https://image.cannabis.wiki/news/istock-1171600388-1573724503-1440w.jpeg" alt="no matches">
    </div>
</div>
</div>
<div id="userPortal" class="container d-none">
    <div id="products" class="d-flex justify-content-center ">
        <div id="iFix" class="card" style="width: 30rem;">
            <img id="defaultImg" class="card-img-top"
                src="https://images.unsplash.com/photo-1579118459333-b6c080d24b5c?ixlib=rb-1.2.1&auto=format&fit=crop&w=634&q=80"
                alt="Card image cap">
            <div class="card-body">
                <h5 id="cardTitle" data-id="" class="card-title">Card title</h5>
                <p id="userDescription" class="card-text">Some quick example text to build on the card title and make up
                    the bulk of the card's content.</p>
                <div id="userweedPref"><b>Weed Preference</b></div><br>
                <a href="#" id="liked" class="btn btn-primary">Like</a>
                <a href="#" id="disliked" class="btn btn-danger">Dislike</a>
            </div>
        </div>
    </div>
</div>

    <script src="/assets/js/ratingUsers.js"></script>
